
material VGD_Alone_lvl_tex_courtyard_bamboo
{
	technique Default
	{
		pass Main
		{
			ambient 0.588 0.588 0.588 1
			specular 0.9 0.9 0.9 1 0.1


			texture_unit
			{
				texture_alias diffuse
				texture VGD_Alone_lvl_tex_courtyard_bamboo_diffuse.dds -1
			}

			texture_unit
			{
				texture_alias self_illumination
				texture courtyard_light_map.png -1
				tex_coord_set 1
			}
		}

	}

}

